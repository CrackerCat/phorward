@IDENT:[A-Za-z_][A-Za-z0-9_]*
@REGEX:/([^\\]|\\/)*/
@STRING:".*"
@colon:

terminal:@@:@IDENT:@REGEX
terminal:@@:@IDENT:@STRING
terminal:@@:@IDENT

nonterminal:@IDENT:@colon:productions
nonterminal:@IDENT

symbol:@@:@IDENT
symbol:@IDENT

production:production:symbol
production:symbol

rhs:production
rhs:

productions:productions:@|:rhs
productions:rhs

definition:terminal:@;
definition:nonterminal:@;

definitions:definitions:definition
definitions:definition

grammar:definitions
grammar:

